@@ -1098,6 +1098,18 @@
     *p = 0;
     return strdup(path);
 }
+
+void tcc_set_lib_path(void *module_handle)
+{
+    static char path[1024];
+    char *p;
+
+    path[0] = '\0'; 
+    GetModuleFileNameA(module_handle, path, sizeof path);
+    p = tcc_basename(normalize_slashes(strlwr(path)));
+    if (p) *p = 0;
+    tcc_lib_path = path;
+}
